import java.util.Scanner;

public class CountZeroes {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();         
        String num = Integer.toString(n);
        int count = 0;

     
        for (int i = 0; i < num.length(); i++) {
            if (num.charAt(i) == '0') {
                count++;
            }
        }

        System.out.println(count); 
    }
}
